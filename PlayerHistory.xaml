<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Project2A.PlayerHistory"
             Title="PlayerHistory">
    <ScrollView>
        <StackLayout x:Name="container1" Spacing="0" Padding="0" WidthRequest="100" VerticalOptions="Start" HorizontalOptions="Start" Margin="10,0,0,0">
            <Label x:Name="roundCounter" VerticalOptions="End" HorizontalOptions="End"></Label>
            <Label x:Name="guessCounter" VerticalOptions="End" HorizontalOptions="End"></Label>
        </StackLayout>
    </ScrollView>
</ContentPage>